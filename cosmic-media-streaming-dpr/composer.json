{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.3",
        "althinect/filament-spatie-roles-permissions": "^2.0",
        "andreiio/blade-remix-icon": "^3.3",
        "arbermustafa/filament-locationpickr-field": "^2.0.0",
        "barryvdh/laravel-dompdf": "^3.0",
        "barryvdh/laravel-snappy": "^1.0",
        "bezhansalleh/filament-shield": "^3.0@beta",
        "calebporzio/sushi": "^2.4",
        "codeat3/blade-element-plus-icons": "^1.0",
        "codeat3/blade-google-material-design-icons": "^1.19",
        "filament/filament": "^3.0.52",
        "guzzlehttp/guzzle": "^7.9",
        "joshembling/image-optimizer": "^1.6",
        "khatabwedaa/blade-css-icons": "^1.4",
        "laravel/framework": "^10.10",
        "laravel/sanctum": "^3.2",
        "laravel/tinker": "^2.8",
        "league/flysystem-aws-s3-v3": "^3.28",
        "livewire/livewire": "^3.0.5",
        "maatwebsite/excel": "^3.1",
        "mikehaertl/phpwkhtmltopdf": "^2.5",
        "pbmedia/laravel-ffmpeg": "^8.6",
        "php-ffmpeg/php-ffmpeg": "^1.3",
        "pion/laravel-chunk-upload": "^1.5",
        "pusher/pusher-php-server": "^7.2",
        "pxlrbt/filament-excel": "^2.3",
        "spatie/laravel-medialibrary": "^11.9"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.15",
        "fakerphp/faker": "^1.9.1",
        "friendsofphp/php-cs-fixer": "^3.40",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "sniff": [
            "./vendor/bin/php-cs-fixer fix -vvv --dry-run --show-progress=dots"
       ],
       "lint": [
           "./vendor/bin/php-cs-fixer fix -vvv --show-progress=dots"
       ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
